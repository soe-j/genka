#!/usr/bin/env bash

RESOURCES_PATH=$(dirname $0)/../resources
CONFIG_PATH=$(dirname $0)/../config

PROJECT_ID=$(cat $RESOURCES_PATH/projects/$1)
STAGES_ID=$(cat $RESOURCES_PATH/stages/$2)
MEMBER_ID=$(cat $CONFIG_PATH/myname.id)

http post localhost:3000/periods project_id=$PROJECT_ID stage_id=$STAGES_ID member_id=$MEMBER_ID | jq .
